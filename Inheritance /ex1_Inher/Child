package ex01_Inheritance;

public class Child extends Parent{
	int x=20;
	
	void varTest() {
		System.out.println(x); // 나에게서 가장 가까운 x를 찾는다 //20
		System.out.println(this.x); // 자식의 x // 20
		System.out.println(super.x); // 부모의 x // 10
		// 자식에서 부모와 자식을 따로 구분 할 수 있다
		
		
		
		int z;
		
//		Child(){ // 생략가능
//			super();
//		}
		
	}
}
